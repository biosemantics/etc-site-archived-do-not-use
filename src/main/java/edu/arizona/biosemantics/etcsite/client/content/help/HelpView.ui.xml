<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:lay="urn:import:edu.arizona.biosemantics.etcsite.client.layout">
	<ui:style>
		
	</ui:style>
	<g:ScrollPanel>
	<lay:CenteredContentPanel>
		<g:HTMLPanel>
		    <g:VerticalPanel>
			    <g:Label>Watch the video tutorial: </g:Label>
			    
			    <g:HTMLPanel>
			    <object ui:field="objectElement">
		            <param ui:field="movieParamElement" name="movie"/>
		            <param ui:field="qualityParamElement" name="quality"/>
		            <param ui:field="bgColorParamElement" name="bgcolor"/>
		            <param ui:field="flashVarsParamElement" name="flashVars"/>
		            <param ui:field="allowFullScreenParamElement" name="allowFullScreen"/>
		            <param ui:field="scaleParamElement" name="scale"/>
		            <param ui:field="allowScriptAccessParamElement" name="allowScriptAccess"/>
		            <param ui:field="baseParamElement" name="base"/>
		        </object>
		        </g:HTMLPanel>
		        
			</g:VerticalPanel>
		</g:HTMLPanel>
	</lay:CenteredContentPanel>
	</g:ScrollPanel>
</ui:UiBinder> 