<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:lay="urn:import:edu.arizona.biosemantics.etcsite.client.layout">
	<ui:style>      
		.label {
			padding-right: 10px;
		}
		.infoImage {
			height: 20px;
			width: 20px;
			padding-right: 5px;
		}
		.panel {
			margin: auto;
			width:950px
		}
	
		.errorLabel {
			color:red;
		}
		.header{
			padding-top:20px;
			font-size: 24px;
			font-weight: bold;
			color: #339900;
		}
	</ui:style>
	<g:ScrollPanel>

	<lay:CenteredContentPanel>
		<g:VerticalPanel>
		    <g:Label styleName="{style.header}">Account Information</g:Label>
		    <g:SimplePanel ui:field="panel" styleName="{style.panel}">
		     
		    </g:SimplePanel>
		</g:VerticalPanel>
	</lay:CenteredContentPanel>
	</g:ScrollPanel>
</ui:UiBinder> 
