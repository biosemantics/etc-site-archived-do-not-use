<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:edu.arizona.biosemantics.etcsite.client.content.semanticMarkup">
	<ui:style>
		.otoFrame {
			border: 0;
			width:	900px;
			height: 500px;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:Label>Please review the terms we learned. 
		You can select terms in the left column and drag them into the categories on the 
		right hand side.
		</g:Label>
		<g:Frame ui:field="frame" styleName="{style.otoFrame}">
		</g:Frame>
		<g:Button ui:field="nextButton">Next</g:Button>
	</g:VerticalPanel>
</ui:UiBinder> 