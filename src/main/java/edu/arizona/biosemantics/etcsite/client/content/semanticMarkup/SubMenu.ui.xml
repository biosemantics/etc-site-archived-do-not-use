<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:edu.arizona.biosemantics.etcsite.client.common">
	<ui:style>
		.subMenuEntry {
			margin-left: 20px;
		}
		.subMenu {
			padding-top: 10px;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:Label styleName="siteHeading">Semantic Markup</g:Label>
		<g:HorizontalPanel styleName="{style.subMenu}">
			<my:ImageLabel ui:field="input" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_1.gif" imageWidth="20px" imageHeight="20px" text="Input"></my:ImageLabel>
			<my:ImageLabel ui:field="preprocess" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_2.gif" imageWidth="20px" imageHeight="20px" text="Preprocess"></my:ImageLabel>
			<my:ImageLabel ui:field="learn" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_3.gif" imageWidth="20px" imageHeight="20px" text="Learn"></my:ImageLabel>
			<my:ImageLabel ui:field="review" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_4.gif" imageWidth="20px" imageHeight="20px" text="Review"></my:ImageLabel>
			<my:ImageLabel ui:field="parse" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_5.gif" imageWidth="20px" imageHeight="20px" text="Parse"></my:ImageLabel>
			<my:ImageLabel ui:field="output" styleName="{style.subMenuEntry}" imagePath="images/Enumeration_unselected_6.gif" imageWidth="20px" imageHeight="20px" text="Output"></my:ImageLabel>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 